<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mina.ksw_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="凯盛国际"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.default" />
            </intent-filter>
        </activity>
        <activity android:name=".HallActivity"></activity>
        <activity android:name=".SignupActivity"></activity>
        <activity android:name=".SuccessActivity"></activity>
        <activity android:name=".Find_Password"></activity>
        <activity android:name=".LiveActivity"></activity>
        <activity android:name=".GamesActivity"></activity>
        <activity android:name=".SlotActivity"></activity>
        <activity android:name=".Customer_Service"></activity>
        <activity android:name=".Customer_Service_Nologin"></activity>

        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>